<Window x:Class="ScheduleTracker.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScheduleTracker.WPF"
        xmlns:controls="clr-namespace:ScheduleTracker.WPF.Controls"
        
        Background="{StaticResource BrushBeige}"
        mc:Ignorable="d"
        Height="450" Width="800"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">

    <Grid>
        <Frame x:Name="Main" NavigationUIVisibility="Hidden"/>
        <Button x:Name="next" Content="Next" HorizontalAlignment="Left" Margin="731,386,0,0" VerticalAlignment="Top" Click="clickNext"/>
    </Grid>
</Window>
